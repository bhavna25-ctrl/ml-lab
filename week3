import matplotlib.pyplot as plt
import numpy as np

# Creating an array of 5 numbers (x-values)
x = np.array([1, 2, 3, 4, 5])

# Creating an array for the corresponding y-values
y = np.array([2, 4, 6, 8, 10])

# Creating a scatter plot
plt.scatter(x, y, color='red', marker='o')  # Red dots with circle markers

# Adding title and labels
plt.title('Basic Scatter Plot')
plt.xlabel('X values')
plt.ylabel('Y values')

# Displaying the plot
plt.show()


import pandas as pd
import numpy as np

# Creating an empty Series (will raise a warning in newer versions of pandas)
ser = pd.Series()
print("Empty Pandas Series: \n", ser)

# Simple array
data = np.array(['g', 'e', 'e', 'k', 's'])
ser = pd.Series(data)
print("\nPandas Series created from NumPy array:\n", ser)
